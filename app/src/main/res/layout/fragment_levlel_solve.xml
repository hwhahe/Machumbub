<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LevlelSolveFragment"
    android:background="@color/background"
    tools:ignore="ExtraText">


    //메인

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/main_frame"
        android:layout_width="match_parent"
        android:layout_height="590dp"
        android:layout_marginTop="56dp"
        android:layout_marginBottom="85dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="단계별로 맞춤법을 공부해봐요"
            android:textColor="#57366A"
            android:textSize="16dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout"
            android:layout_width="match_parent"
            android:layout_height="450dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="350dp"
                android:layout_height="350dp"
                android:paddingTop="30dp"
                android:paddingBottom="30dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toTopOf="@+id/constraintLayout2"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent">

                    <Button
                        android:id="@+id/level1"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_weight="1"
                        android:background="#00ff0000"
                        android:drawableTop="@drawable/box"
                        android:fontFamily="@font/inter_medium"
                        android:text="레벨 1"
                        android:textColor="#9E64BF"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toLeftOf="@+id/level2"
                        app:layout_constraintTop_toTopOf="parent" />

                    <Button
                        android:id="@+id/level2"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_weight="1"
                        android:background="#00ff0000"
                        android:drawableTop="@drawable/box"
                        android:fontFamily="@font/inter_medium"
                        android:text="레벨 2"
                        android:textColor="#9E64BF"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toRightOf="@id/level1"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/constraintLayout2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/constraintLayout1">

                    <Button
                        android:id="@+id/level3"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_weight="1"
                        android:background="#00ff0000"
                        android:drawableTop="@drawable/box"
                        android:fontFamily="@font/inter_medium"
                        android:text="레벨 3"
                        android:textColor="#9E64BF"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toLeftOf="@id/level4"
                        app:layout_constraintTop_toTopOf="parent" />

                    <Button
                        android:id="@+id/level4"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_weight="1"
                        android:background="#00ff0000"
                        android:drawableTop="@drawable/box"
                        android:fontFamily="@font/inter_medium"
                        android:text="레벨 4"
                        android:textColor="#9E64BF"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toRightOf="@id/level3"
                        app:layout_constraintRight_toLeftOf="@+id/level5"
                        app:layout_constraintTop_toTopOf="parent" />

                    <Button
                        android:id="@+id/level5"
                        android:layout_width="110dp"
                        android:layout_height="110dp"
                        android:layout_weight="1"
                        android:background="#00ff0000"
                        android:drawableTop="@drawable/box"
                        android:fontFamily="@font/inter_medium"
                        android:text="레벨 5"
                        android:textColor="#9E64BF"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintLeft_toRightOf="@id/level4"
                        app:layout_constraintRight_toRightOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    //하단탭

</androidx.constraintlayout.widget.ConstraintLayout>